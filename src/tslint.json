{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "ak",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "ak",
            "kebab-case"
        ],
        "variable-name": {
            "options": [
                "allow-leading-underscore"
            ]
        }
    }
}